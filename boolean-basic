#include <stdio.h>

int main(void) {
  
  int TRUE = 1;
  int FALSE = 0;

  /*program*/
  printf("Ini adalah nilai True and True =  %d \n",
  TRUE && TRUE);
  printf("Ini adalah nilai True or False = %d \n", TRUE || FALSE);
  printf("ini adalah nilai false and true = %d \n",TRUE && FALSE);
  printf("Ini adalah nilai False and False = %d \n", FALSE && FALSE);

  printf("ini adalah nilai Not True = %d \n", !TRUE);
  printf("Ini adalah nilai Not False = %d \n", !FALSE);

  return 0;

}
